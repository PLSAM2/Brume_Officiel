					#endif
				#endif
				float4 color11 = IsGammaSpace() ? float4(0,0,0,0) : float4(0,0,0,0);
				float2 appendResult23 = (float2(WorldPosition.x , WorldPosition.z));
				float simplePerlin2D42 = snoise( appendResult23*0.1 );
				simplePerlin2D42 = simplePerlin2D42*0.5 + 0.5;
				float4 temp_cast_0 = (0.0).xxxx;
				float4 temp_cast_1 = (1.0).xxxx;
				float4 temp_cast_2 = (0.5).xxxx;
				float4 temp_cast_3 = (1.0).xxxx;
				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
				float dotResult46 = dot( ase_worldNormal , float3( 0,1,0 ) );
				float dotResult4 = dot( ase_worldNormal , float3(0,1,0) );
				float smoothstepResult26 = smoothstep( _StepSnow , ( _StepSnow + _SmoothstepSnow ) , dotResult4);
				float2 temp_cast_4 = (_NoiseTiling).xx;
				float2 texCoord34 = IN.ase_texcoord4.xy * temp_cast_4 + _NoiseOffset;
				float4 tex2DNode28 = tex2D( _Noise, texCoord34 );
				float smoothstepResult31 = smoothstep( 0.0 , 0.25 , ( smoothstepResult26 - tex2DNode28.r ));
				float clampResult8 = clamp( ( smoothstepResult31 - step( tex2DNode28.r , 0.34 ) ) , 0.0 , 1.0 );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( color11 + ( ( ( (0.5 + (simplePerlin2D42 - 0.0) * (1.0 - 0.5) / (1.0 - 0.0)) * (temp_cast_2 + (tex2D( _SnowTexture, appendResult23 ) - temp_cast_0) * (temp_cast_3 - temp_cast_2) / (temp_cast_1 - temp_cast_0)) ) * dotResult46 ) * clampResult8 ) ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
