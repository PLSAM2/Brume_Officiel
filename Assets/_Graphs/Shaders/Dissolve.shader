				float2 _Vector0 = float2(-0.5,0);
				float2 temp_cast_0 = (0.5).xx;
				float2 texCoord9 = IN.ase_texcoord3.xy * temp_cast_0 + float2( 0,0 );
				float2 panner7 = ( 1.0 * _Time.y * _Vector0 + texCoord9);
				float2 temp_cast_1 = (0.5).xx;
				float2 texCoord20 = IN.ase_texcoord3.xy * temp_cast_1 + float2( 0.5,0.5 );
				float2 panner14 = ( 1.0 * _Time.y * ( _Vector0 + float2( 0.2,0 ) ) + texCoord20);
				float4 temp_output_19_0 = ( tex2D( _blurclouds, panner7 ) / tex2D( _blurclouds1, panner14 ) );
				float grayscale30 = Luminance(step( temp_output_19_0 , float4( 1,1,1,0 ) ).rgb);
				float Mask38 = grayscale30;
				float4 color36 = IsGammaSpace() ? float4(1,0,0,0) : float4(1,0,0,0);
				float4 temp_cast_3 = (_Edges).xxxx;
				float grayscale34 = Luminance(step( temp_output_19_0 , temp_cast_3 ).rgb);
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ( Mask38 * color36 ) + grayscale34 ).rgb;
				float Alpha = Mask38;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

