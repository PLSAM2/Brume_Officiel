				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 temp_cast_0 = (_TilingTex).xx;
				float2 texCoord28 = IN.ase_texcoord7.xy * temp_cast_0 + float2( 0,0 );
				float2 panner30 = ( 1.0 * _Time.y * _VitessePanner + texCoord28);
				float2 PanningSpeed31 = panner30;
				float4 tex2DNode32 = tex2D( _Texture1, PanningSpeed31 );
				float smoothstepResult36 = smoothstep( 0.45 , 0.05 , tex2DNode32.r);
				float smoothstepResult46 = smoothstep( 1.58 , 0.05 , tex2DNode32.r);
				float fresnelNdotV55 = dot( WorldNormal, WorldViewDirection );
				float fresnelNode55 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV55, _FresnelStrenghtAlbedo ) );
				float temp_output_56_0 = ( 1.0 - fresnelNode55 );
				float clampResult59 = clamp( ( temp_output_56_0 * temp_output_56_0 * _FresnelIntensityAlbedo ) , 0.0 , 1.0 );
				float4 CaptureAlbedo40 = ( ( ( smoothstepResult36 * _ColorBase2 ) + ( smoothstepResult46 * _ColorBase1 ) ) * clampResult59 );
				
				float2 texCoord11 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner13 = ( _Apparition * float2( 0,-1 ) + texCoord11);
				float fresnelNdotV17 = dot( WorldNormal, WorldViewDirection );
				float fresnelNode17 = ( 0.0 + 1.0 * pow( 1.0 - fresnelNdotV17, _FresnelStrenght ) );
				float temp_output_18_0 = ( 1.0 - fresnelNode17 );
				float clampResult21 = clamp( ( temp_output_18_0 * temp_output_18_0 * 2.0 ) , 0.0 , 1.0 );
				float temp_output_22_0 = ( tex2D( _Texture0, panner13 ).g * clampResult21 );
				float CaptureOpacity42 = ( temp_output_22_0 + ( ( 1.0 - temp_output_22_0 ) * _Opacity1 ) );
				
