				float2 temp_cast_3 = (0.5).xx;
				float2 texCoord50 = IN.ase_texcoord3.xy * temp_cast_3 + float2( 0,0 );
				float2 panner51 = ( 1.0 * _Time.y * _PannerSpeed1 + texCoord50);
				float4 temp_cast_4 = (tex2D( _blurclouds1, panner51 ).r).xxxx;
